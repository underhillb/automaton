
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
<%= devise_error_messages! %>

<div class="form-group">
  <label>First Name</label>

  <%= f.text_field :first_name, autofocus: true,placeholder: "First Name",class: "form-control" %>

  <label>Last Name</label>

  <%= f.text_field :last_name, autofocus: true,placeholder: "Last Name",class: "form-control" %>
</div>

<div class="form-group">

  <label>Email address</label>

  <%= f.email_field :email, autofocus: true,placeholder: "Email",class: "form-control" %>
</div>
<div class="form-group">
  <label>Password</label>
  <%= f.password_field :password, class: "form-control",autocomplete: "off", placeholder: "Password" %>
</div>
<div class="form-group">
  <label>Password Confirmation</label>
  <%= f.password_field :password_confirmation, class: "form-control",autocomplete: "off", placeholder: "Password Confirmation" %>
</div>
<button type="Sign up" class="btn btn-primary">Submit</button>
<% end -%>

<%= render "devise/shared/links" %>
